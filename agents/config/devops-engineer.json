{
  "$schema": "https://raw.githubusercontent.com/aws/amazon-q-developer-cli/refs/heads/main/schemas/agent-v1.json",
  "name": "devops-engineer",
  "description": "sub-agent:Expert in deployment, CI/CD, infrastructure, monitoring, and production operations. Use for: deployments, pipeline management, rollbacks, monitoring setup.",
  "prompt": null,
  "mcpServers": {},
  "tools": [
    "*"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "fs_write",
    "execute_bash",
    "use_aws"
  ],
  "resources": [
    "file://{{KIRO_DIR}}/sub-agents/prompts/devops-engineer.md"
  ],
  "hooks": {
    "agentSpawn": [
      {
        "command": "{{HOOK_NOTIFY}} --agent-name \"devops-engineer\"",
        "timeout_ms": 10000,
        "max_output_size": 1024,
        "cache_ttl_seconds": 0
      }
    ]
  },
  "toolsSettings": {},
  "useLegacyMcpJson": true
}
